{
  "name": "my-v0-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --webpack",
    "build": "next build",
    "codegen": "graphql-codegen --config codegen.ts",
    "codegen:watch": "graphql-codegen --config codegen.ts --watch",
    "start": "next start",
    "lint": "eslint .",
    "db:seed": "npx tsx scripts/seed.ts",
    "db:seed:reset": "npx tsx scripts/seed.ts --reset",
    "db:seed:minimal": "npx tsx scripts/seed.ts --minimal",
    "db:seed:json": "npx tsx scripts/seed-from-json.ts",
    "db:seed:json:reset": "npx tsx scripts/seed-from-json.ts --reset",
    "hasura:console": "cd hasura && hasura console --admin-secret ${HASURA_GRAPHQL_ADMIN_SECRET:-hasura_admin_secret_change_in_production}",
    "hasura:migrate:apply": "cd hasura && hasura migrate apply --admin-secret ${HASURA_GRAPHQL_ADMIN_SECRET:-hasura_admin_secret_change_in_production}",
    "hasura:migrate:status": "cd hasura && hasura migrate status --admin-secret ${HASURA_GRAPHQL_ADMIN_SECRET:-hasura_admin_secret_change_in_production}",
    "hasura:metadata:apply": "cd hasura && hasura metadata apply --admin-secret ${HASURA_GRAPHQL_ADMIN_SECRET:-hasura_admin_secret_change_in_production}",
    "hasura:metadata:export": "cd hasura && hasura metadata export --admin-secret ${HASURA_GRAPHQL_ADMIN_SECRET:-hasura_admin_secret_change_in_production}",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:backend": "vitest run tests/**/*.test.ts",
    "test:component": "vitest run src/**/*.test.tsx",
    "test:component:ui": "vitest --ui src/**/*.test.tsx",
    "test:docker": "vitest run",
    "test:docker:ui": "vitest --ui",
    "test:docker:component": "vitest run --include 'src/**/*.test.tsx'",
    "test:docker:e2e": "playwright test",
    "test:docker:all": "npm run test:docker && npm run test:docker:component",
    "test:docker:setup": "docker-compose exec db psql -U myuser -d postgres -c \"CREATE DATABASE test_lab_crm;\" || true",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:all": "npm run test:run && npm run test:e2e",
    "seed:docker:reset": "docker-compose exec app sh -c 'npm run db:seed:reset'"
  },
  "dependencies": {
